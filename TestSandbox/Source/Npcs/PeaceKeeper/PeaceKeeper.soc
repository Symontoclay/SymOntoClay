app PeaceKeeper
{
    on Enter =>
    {
        'Begin' >> @>log;
        'End' >> @>log;
    }

    fun SomeFun()
    {
        'Begin SomeFun' >> @>log;
        return @b;
    }

    var @b;
    var @a = 15;

	on {: see(I, #a) :} duration 1 (down) => 
    {
	    'S' >> @>log;
        @a = 5;
        @b = 1;
	}

    on (SomeFun())
    {
        'D' >> @>log;
    }
}
