app PeaceKeeper
{
    fun a(@param_1)
    {
        '`a` (1) has been called!' >> @>log;
        @param_1 >> @>log;
    }

    fun a(@param_1, @param_2)
    {
        '`a` (2) has been called!' >> @>log;
        @param_1 >> @>log;
        @param_2 >> @>log;
    }

    on Enter =>
    {
        'Begin' >> @>log;
        @param_1 = 12;

        a(@param_1);
        a(@param_1: 3, @param_2: 'Hi');
        'End' >> @>log;
    }

