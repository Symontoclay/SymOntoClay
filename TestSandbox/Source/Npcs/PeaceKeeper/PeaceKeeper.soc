linvar age for range (0, 150]
{
    terms:
	    `teenager` = Trapezoid(10, 12, 17, 20);
}

app PeaceKeeper
{
    on Init =>
    {
        'Begin' >> @>log;
        
        @a = 30;

        if(@a is teenager)
        {
            'Yes!' >> @>log;
        } else {
            'Else Yes!' >> @>log;
        }

        'End' >> @>log;
    }
}
