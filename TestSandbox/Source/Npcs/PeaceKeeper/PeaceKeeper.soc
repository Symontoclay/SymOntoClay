app PeaceKeeper
{
    on Enter =>
    {
        'Begin' >> @>log;
        Fun1();
        Fun2();
        'SomeProp:' >> @>log;
        SomeProp >> @>log;
        'End' >> @>log;
    }

    fun Fun1()
    {
        'Run Fun1' >> @>log;
        'Before SomeProp:' >> @>log;
        SomeProp >> @>log;
        'After SomeProp:' >> @>log;
        SomeProp >> @>log;
    }

    fun Fun2()
    {
        'Run Fun1' >> @>log;
        'SomeProp:' >> @>log;
        SomeProp >> @>log;
    }
}